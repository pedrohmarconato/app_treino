{
  "name": "app-treino",
  "version": "5.6.1",
  "description": "Aplicação web completa para gerenciamento de treinos físicos",
  "keywords": [
    "fitness",
    "workout",
    "training",
    "health",
    "exercise",
    "gym",
    "progressive-web-app",
    "javascript"
  ],
  "homepage": "https://github.com/pedrohmarconato/app_treino#readme",
  "bugs": {
    "url": "https://github.com/pedrohmarconato/app_treino/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pedrohmarconato/app_treino.git"
  },
  "license": "MIT",
  "author": {
    "name": "Pedro Henrique Marconato",
    "email": "pedrohenriquemarconato@gmail.com",
    "url": "https://github.com/pedrohmarconato"
  },
  "contributors": [
    {
      "name": "Claude AI",
      "email": "noreply@anthropic.com",
      "url": "https://claude.ai"
    }
  ],
  "main": "index.html",
  "scripts": {
    "start": "serve -s . -l 3000",
    "dev": "serve -s . -l 3000 --cors",
    "build": "npm run minify:css && npm run minify:js && node build.js",
    "preview": "serve -s dist -l 4173",
    "test": "echo 'No tests configured yet'",
    "test:watch": "echo 'No tests configured yet'",
    "test:coverage": "echo 'No tests configured yet'",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "minify:css": "cleancss -o dist/styles.min.css styles.css",
    "minify:js": "terser js/app.js -o dist/app.min.js --config-file terser.config.json",
    "security:audit": "npm audit --audit-level moderate || true",
    "security:fix": "npm audit fix || true",
    "deps:check": "ncu || true",
    "deps:update": "ncu -u && npm install",
    "clean": "rimraf dist coverage .nyc_output",
    "analyze": "echo 'No bundle analysis configured'",
    "prepare": "husky install || true",
    "release": "standard-version",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-jest": "^29.6.0",
    "clean-css-cli": "^5.6.0",
    "cypress": "^13.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.13.3",
    "gh-pages": "^6.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.0",
    "jest-environment-jsdom": "^29.6.0",
    "npm-check-updates": "^16.10.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "serve": "^14.2.0",
    "standard-version": "^9.5.0",
    "terser": "^5.19.0",
    "typescript": "^5.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "bundlesize": [
    {
      "path": "./dist/*.js",
      "maxSize": "100 kB"
    },
    {
      "path": "./dist/*.css",
      "maxSize": "50 kB"
    }
  ],
  "type": "module",
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off",
      "prefer-const": "error",
      "no-var": "error"
    },
    "ignorePatterns": [
      "dist/",
      "coverage/",
      "node_modules/"
    ]
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 100
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format:check",
      "pre-push": "npm run test && npm run build"
    }
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0"
  }
}

